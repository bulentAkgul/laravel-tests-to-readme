{
  "name": "bakgul/laravel-tests-to-readme",
  "description": "A package to output test methods to readme files as a documantation.",
  "authors": [
    {
      "name": "Bulent Akgul",
      "email": "bulent.akgul@me.com"
    }
  ],
  "require": {
    "bakgul/laravel-helpers": "^1.0"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Bakgul\\LaravelTestsToReadme\\": "src/",
      "Bakgul\\LaravelTestsToReadme\\Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bakgul\\LaravelTestsToReadme\\ToReadmeServiceProvider"
      ]
    }
  }
}